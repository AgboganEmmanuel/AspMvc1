@model IEnumerable<TP_Vues_Controller.Models.IClients> 

@{
    ViewBag.Title = "Clientsgi";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<table class="table">
    <thead class="thead-dark">
        <tr>

            <th scope="col">@Html.DisplayNameFor(model => model.Id)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Nom)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Couriel)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.NoTelephone)</th>


        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Couriel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoTelephone)
            </td>
        </tr>
        }


    </tbody>
</table>


